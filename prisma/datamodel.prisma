type Burger {
    id: ID! @id
    createdAt: DateTime! @createdAt
    description: String!,
    restaurant: String!,
    name: String!,
    rating: Int!,
    postedBy: User
}

type User {
    id: ID! @id,
    name: String!,
    email: String! @unique,
    password: String!,
    burgers: [Burger!]!,
}