type Burger {
    id: ID! @id
    createdAt: DateTime! @createdAt
    description: String!
    restaurant: String!
    name: String!
    rating: Int!
    postedBy: User
    votes: [Vote!]!
}

type User {
    id: ID! @id
    name: String!
    email: String! @unique
    password: String!
    burgers: [Burger!]!
    votes: [Vote!]!

}

type Vote {
    id: ID! @id
    burger: Burger!
    user: User!
}